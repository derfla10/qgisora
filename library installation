# To install a python library in the QGIS Windows python environment:
# To find the old MS-DOS 8.3 convention path use DIR /x on the directory of your windows system

set HTTPS_PROXY=http://username:password@proxyname:proxyport
set  HTTP_PROXY=http://username:password@proxyname:proxyport
set PYTHONPATH=C:\PROGRA~1\QGIS32~1.1\apps\Python39
set PYTHONHOME=C:\PROGRA~1\QGIS32~1.1\apps\Python39


C:\Program Files\QGIS 3.28.1\apps\Python39\Scripts>pip3.9.exe install cx_Oracle 

